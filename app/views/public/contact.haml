    / .SEC_MENU
    .content
      .secondary_pages_content
        %h1 Have a project for us?
        %p.subtitle Get in touch and we'll send you a quote
        %p Lorem Ipsum is simply dummy text of the printing and typesetting industry. Lorem Ipsum has been the industry's standard dummy text. Ever since the 1500s, when an unknown printer took. Lorem Ipsum is simply dummy text of the printing and typesetting industry. Lorem Ipsum has been the industry's standard dummy text. Ever since the 1500s, when an unknown printer took.
        .contact_box
          .contact_box_top
          .contact_box_middle
            %form.ajax_form{:action => "", :enctype => "multipart/form-data", :method => "post"}
              %table.contact_form{:cellpadding => "0", :cellspacing => "0"}
                %tr
                  %td{:style => "width:461px"}
                    %label{:for => "name"} Your name (required):
                    .form_input.odd
                      .input_left
                      %input#name.input_middle.required{:name => "yourname", :type => "text"}/
                      .input_right
                  %td{:style => "width:461px"}
                    %label{:for => "email"} Your email (required):
                    .form_input
                      .input_left
                      %input#email.input_middle.required{:name => "email", :type => "text"}/
                      .input_right
                %tr
                  %td{:style => "width:461px"}
                    %label{:for => "phone"} Your phone number:
                    .form_input.odd
                      .input_left
                      %input#phone.input_middle{:name => "phone", :type => "text"}/
                      .input_right
                  %td{:style => "width:461px"}
                    %label{:for => "company"} Your company:
                    .form_input
                      .input_left
                      %input#company.input_middle{:name => "company", :type => "text"}/
                      .input_right
                %tr
                  %td{:style => "width:461px"}
                    %label{:for => "budget"} Your budget:
                    .form_input.odd
                      .input_left
                      %input#budget.input_middle{:name => "Budget", :type => "text"}/
                      .input_right
                  %td{:style => "width:461px"}
                    %label{:for => "deadline"} Deadline:
                    .form_input
                      .input_left
                      %input#deadline.input_middle{:name => "Deadline", :type => "text"}/
                      .input_right
                %tr
                  %td{:colspan => "2", :style => "width:922px"}
                    %label{:for => "deadline"} Your message:
                    .form_textarea
                      .textarea_left
                      %textarea#message.textarea_middle.required{:cols => "103", :name => "message", :rows => "5"}
                      .textarea_right
                %tr
                  %td{:colspan => "2", :style => "width:922px"}
                    %input#send.submit{:type => "submit", :value => ""}/
          .contact_box_bottom
        / contact_box
      / secondary_pages_content
      .clear_container
      %br/
      %br/
    / .CONTENT
    .footer
      .footer_content
        .footer_left
          %p SITE DESIGN Â© 2009-2010 Creative juice, inc&nbsp;&nbsp;&nbsp;|&nbsp;&nbsp;&nbsp;all rights reserved
          %a.rss{:href => ""} subscribe to our rss feed
        / .footer_left
        .footer_right
          %p
            %a{:href => "#"}> privecy statement
            \&nbsp;&nbsp;&nbsp;|&nbsp;&nbsp;&nbsp;
            %a{:href => "#"} terms &amp; conditions
        / .footer_right
      / .footer_content
    / .FOOTER
