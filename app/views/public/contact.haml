!!! Strict
%html{:xmlns => "http://www.w3.org/1999/xhtml"}
  %head
    %meta{:content => "text/html; charset=utf-8", "http-equiv" => "Content-Type"}/
    %title Creative Portfolio - Contact
    %meta{:content => "WebCrafters", :name => "author"}/
    %meta{:content => "A short description of your company", :name => "description"}/
    %meta{:content => "Some keywords that best describe your business. Try to use these words in the texts from the page", :name => "keywords"}/
    %meta{:content => "english", :name => "language"}/
    %link{:href => "images/favicon.ico", :rel => "icon", :type => "image/ico"}/
    %link{:href => "css/global.css", :rel => "stylesheet", :type => "text/css"}/
    %link{:href => "css/color_blue.css", :rel => "stylesheet", :type => "text/css"}/
    %script{:src => "js/jquery-1.3.2.min.js", :type => "text/javascript"}
    %script{:src => "js/cufon-yui.js", :type => "text/javascript"}
    %script{:src => "js/custom.font.js", :type => "text/javascript"}
    %script{:src => "js/custom.js", :type => "text/javascript"}
    /[if IE 6] <link type="text/css" rel="stylesheet" href="css/ie6.css" />
  %body
    .header
      .header_content
        %a{:href => "/"}
          %img.logo.png{:alt => "DZKey Logo", :height => "35", :width => "140", :src => "images/logo.png", :title => "Go to Homepage"}/
        %ul.main_menu
          %li
            %a.homepage{:href => "/"}
          %li
            %a{:href => "/tour"} Feature Tour
          %li
            %a{:href => "/faq"} FAQ
          %li.selected_page
            .selected
              .selected_left
              .selected_middle
                %a{:href => "/contact"} Contact
              .selected_right
          %li
            %a{:href => "/login"} Log In
          %li
            %a.awesome.medium.green{:href => "/signup", :style => "color: white; margin-top: -8px;"} Sign Up
    .secondary_page_bg
      .category_illustration
        .punchline_contact
          %h2 Please contact us using the form below
          %p We answer every single message. We promise.
        %img.phone{:alt => "Contact", :height => "159", :src => "images/phone.png", :width => "216"}/
      / .project
    / .LATEST_PROJECT
    .sec_menu
      .sec_menu_content
        %ul.secondary_menu.secondary_menu_contact
          %li
            %span Our Address
            &nbsp;
            557 N 17th Street &nbsp; Milwaukee, WI
          %li
            %span Phone
